<h1> New User</h1>
<%= form_for :user, url: user_path do |f| %>
  <p>
    <%= f.label :Name %><br>
    <%= f.text_field :Name %>
  </p>
 
  <p>
    <%= f.label :Username %><br>
    <%= f.text_field :Username %>
  </p>
  
  <p>
    <%= f.label :Email %><br>
    <%= f.text_field :Email %>
  </p
  
  
 
  <p>
    <%= f.submit %>
  </p>
<% end %>
