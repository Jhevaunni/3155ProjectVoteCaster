<h1>Poll Listing</h1>
<%= link_to 'Create New Poll', new_poll_url %>

<table>
    <tr>
        <th>Poll Description</th>
    </tr>
    
    <% @polls.each do |polls| %>
    <tr>
        <td><%=polls.Poll_contents %></td>
        <td><%= link_to 'Show', polls_path(polls) %></td>
    </tr>
    
    <%end%>
    
</table>